    name="base_link">
    <inertial>
      <origin
        xyz="0.0001429 -0.00085582 0.01685"
        rpy="0 0 0" />
      <mass
        value="1.65" />
      <inertia
        ixx="0.014959"
        ixy="0.0"
        ixz="0.0"
        iyy="0.022005"
        iyz="0.0"
        izz="0.028033" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://mavros_off_board/meshes/quad_f450/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.75294 0.75294 0.75294 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <box size="0.3585 0.3585 0.22"/>
      </geometry>
    </collision>
  </link>
  <!-- gazebo reference of the base_link -->
  <gazebo reference="base_link">
    <material>Gazebo/DarkGrey</material>
    <turnGravityOff>false</turnGravityOff>
  </gazebo>

  <!-- IMU link -->
  <link
    name="/imu_link">
    <inertial>
      <origin
        xyz="0.0 0.0 0.0"
        rpy="0 0 0" />
      <mass
        value="0.015" />
      <inertia
        ixx="1E-05"
        ixy="0.0"
        ixz="0.0"
        iyy="1E-05"
        iyz="0.0"
        izz="1E-05" />
