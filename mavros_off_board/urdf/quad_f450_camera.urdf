    name="camera_joint"
    type="fixed">
    <origin
      xyz="0 0 -0.013633"
      rpy="1.5708 1.5708 0" />
    <parent
      link="base_link" />
    <child
      link="camera_link" />
    <axis
      xyz="-1 0 0" />
  </joint>

