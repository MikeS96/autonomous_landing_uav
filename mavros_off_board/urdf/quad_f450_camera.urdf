    <turnGravityOff>false</turnGravityOff>
  </gazebo>

  <!-- IMU joint -->
  <joint
    name="/imu_joint"
    type="revolute">
    <origin
      xyz="0 0 0"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="/imu_link" />
    <axis
      xyz="1 0 0" />
    <limit
      lower="0"
      upper="0"
      effort="0"
      velocity="0" />
  </joint>

